{
  "name": "devpark/ab",
  "description": "Projects management application.",
  "keywords": [
    "devpark"
  ],
  "type": "project",
  "require": {
    "php": ">=7.4.0",
    "ext-json": "*",
    "doctrine/dbal": "^2.5",
    "fideloper/proxy": "^4.0",
    "grimzy/laravel-mysql-spatial": "^5.0",
    "gusapi/gusapi": "^3.0",
    "guzzlehttp/guzzle": "7.*",
    "intervention/image": "^2.3",
    "laravel/framework": "8.*",
    "laravel/helpers": "^1.4",
    "laravel/legacy-factories": "^1.1",
    "laravel/slack-notification-channel": "^2.2",
    "laravel/telescope": "^4.10",
    "laravel/tinker": "^2.5",
    "maatwebsite/excel": "^3.0",
    "niklasravnsborg/laravel-pdf": "^4.0",
    "openpayu/openpayu": "^2.2",
    "predis/predis": "^1.1",
    "spatie/laravel-http-logger": "^1.8",
    "steffjenl/laravel-azure-blob-storage": "^1.0",
    "tymon/jwt-auth": "^1.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.4",
    "dms/phpunit-arraysubset-asserts": "^0.2.1",
    "filp/whoops": "~2.0",
    "friendsofphp/php-cs-fixer": "^3.3",
    "fzaninotto/faker": "~1.4",
    "laravel/browser-kit-testing": "^6.2",
    "mockery/mockery": "1.3.*",
    "phpunit/phpunit": "^9.5.11",
    "wnx/laravel-stats": "^2.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers/helpers.php",
      "vendor/openpayu/openpayu/lib/openpayu.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "fixer": "vendor/bin/php-cs-fixer fix"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
